<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        {% block metadata %}
            <meta charset="utf-8">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
            <meta name="description" content="">
            <meta name="author" content="">
        {% endblock %}
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('jquery_components/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('jquery_components/bootstrap/dist/css/bootstrap-theme.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/style.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
            {% block menu %}
                <nav class="navbar navbar-fixed-top">
                    <div class="container-fluid">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="{{ path('homepage') }}"><img class="img-responsive img-circle logo" src="{{ asset('bundles/app/images/logo.png') }}"></a>
                        </div>

                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                {#{{ knp_menu_render('AppBundle:Builder:topMenu', { 'template': 'Menu/knp_menu.html.twig', 'currentAsLink': false, 'id':'navigation'}) }}#}
                                <li class="active"><a href="{{ path('homepage') }}">Home<span class="sr-only">(current)</span></a></li>
                                <li><a href="{{ path('about_us') }}">About Us</a></li>
                                <li><a href="{{ path('why_directrex') }}">Why Direct From US</a></li>
                                <li><a href="{% if app.user == null %}{{ path('products') }}
                                {% else %}{{ path('list') }}{% endif %}">Products</a></li>
                                <li><a href="{{ path('services') }}">Services</a></li>
                                {% if app.user == null %}
                                <li><a href="{{ path('fos_user_security_login') }}">Login</a></li>
                                    {% else %}
                                        <li><a href="{{ path('user_settings') }}">Credit Application</a></li>
                                        <li><a href="{{ path('my_bag') }}" class="selected_infos">My&nbsp;Bag&nbsp;</a></li>
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span class="glyphicon glyphicon-user"></span> &nbsp;{{ app.user.lastname }}<span class="caret"></span></a>
                                            <ul class="dropdown-menu">
                                                {% if is_granted('ROLE_SUPER_ADMIN') %}
                                                    <li><a href="{{ path('sonata_admin_dashboard') }}"><span class="glyphicon glyphicon-book">&nbsp;Dashboard</span></a></li>
                                                {% endif %}
                                                <li><a href="{{ path('submit_order') }}"><span class="glyphicon glyphicon-screenshot"></span>&nbsp;Orders</a></li>
                                                <li><a href="{{ path('fos_user_profile_edit') }}"><span class="glyphicon glyphicon-edit"></span>&nbsp;Edit Profile</a></li>
                                                <li role="separator" class="divider"></li>
                                                <li><a href="{{ path('fos_user_profile_show') }}"><span class="glyphicon glyphicon-eye-open"></span>&nbsp;Profile</a></li>
                                                <li role="separator" class="divider"></li>
                                                <li><a href="{{ path('fos_user_security_logout') }}"><span class="glyphicon glyphicon-log-out"></span>&nbsp;Logout</a></li>
                                            </ul>
                                        </li>
                                {% endif %}
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </div><!-- /.container-fluid -->
                </nav>
            {% endblock %}
            {% block banner %}
                <div id="slider">
                    <figure>
                        <img src="https://media.licdn.com/mpr/mpr/AAEAAQAAAAAAAAl6AAAAJDg2OTlkMzc3LTA5NzMtNDc1OS04MzYxLWMzMjM5MmIwYTg1Zg.jpg" alt>
                        <img src="https://media.licdn.com/mpr/mpr/AAEAAQAAAAAAAAkbAAAAJDEzMjgxY2NkLTMyM2ItNGFmMC1hOGJlLTU4M2JiOGJhMzdkNg.jpg" alt>
                        <img src="https://media.licdn.com/mpr/mpr/AAEAAQAAAAAAAAibAAAAJDhlM2UwNDc3LWNkY2QtNGJiNy04Y2FhLThkZDJlYTJjMjhkNA.jpg" alt>
                        <img src="https://media.licdn.com/mpr/mpr/AAEAAQAAAAAAAAgCAAAAJDY1ZGVjZjQ4LTc1ZjQtNDQ2YS04ZjFmLWIxZTM0NjE1NjU2Ng.jpg" alt>
                    </figure>
                </div>
            {% endblock %}
            {% block content %}
            {% endblock %}
            {% block footer %}
                <footer class="footer">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-4 col-xs-6">
                                <ul class="footer-info-list">
                                    <li>AAROGYA MEDSURG LLC</li>
                                    <li><span>Toll Free:</span>1-888-888-8888</li>
                                    <li><span>Fax:</span>1-888-888-8888</li>
                                    <li><span>Cell:</span>1-909-682-8477</li>
                                    <li><span>Tel:</span>1-909-980-9900</li>
                                </ul>
                            </div>
                            <div class="col-md-4 col-xs-6">
                                <ul class="footer-info-list">
                                    <li>Working hours Mon to Fri 9-00am to 5-00pm</li>
                                    <li><span>Address:</span>8351 ELM AVE, #103</li>
                                    <li><span>City:</span>RANCHO CUCAMONGA</li>
                                    <li><span>State:</span>CA</li>
                                    <li><span>Zip:</span>91730</li>
                                </ul>
                            </div>
                            <div class="col-md-4 col-xs-12">
                                <ul class="footer-info-image">
                                    <li><p>Email: info@aamedllc.com</p></li>
                                </ul>
                                <ul class="footer-info-image">
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-rounded" src="{{ asset('bundles/app/images/certificates/1.png') }}">
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-thumbnail" src="{{ asset('bundles/app/images/certificates/2.png') }}">
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-thumbnail" src="{{ asset('bundles/app/images/certificates/3.png') }}">
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-thumbnail" src="{{ asset('bundles/app/images/certificates/4.png') }}">
                                        </a>
                                    </li>
                                </ul>

                            </div>
                        </div>
                    </div>
                </footer>
            {% endblock %}
        {% endblock %}
        <script type="text/javascript" src="{{ asset('jquery_components/jquery/dist/jquery.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('jquery_components/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        <script type="text/javascript">
            var text = $('.dropdown-toggle').text();
            $('.dropdown-toggle').html('<span class="glyphicon glyphicon-user"></span> &nbsp;'+text+'&nbsp;<span class="caret"></span>')
        </script>
        {% block javascripts %}
        {% endblock %}
    </body>
</html>
