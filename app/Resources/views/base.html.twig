<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        {% block metadata %}
            <meta charset="utf-8">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
            <meta name="title" content="AAROGYA MEDSURG LLC is a Pharmacy - Wholesalers licensed to practice in California.">
            <meta name="description" content="AAROGYA MEDSURG LLC is a Pharmacy - Wholesalers licensed to practice in California. Our management team has a combined years of commitment to quality.">
            <meta name="author" content="AAROGYA MEDSURG LLC ">
        {% endblock %}
        <title>{% block title %}{% if name %}{{ name }}{% else %}AAROGYA MEDSURG LLC {% endif %}{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('jquery_components/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('jquery_components/bootstrap/dist/css/bootstrap-theme.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/style.css') }}">
        {% endblock %}
        <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('bundles/app/images/favicon/apple-icon-57x57.png') }}">
        <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('bundles/app/images/favicon/apple-icon-60x60.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-72x72.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-76x76.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-114x114.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-120x120.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-144x144.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-152x152.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/apple-icon-180x180.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/android-icon-192x192.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/favicon-32x32.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/favicon-96x96.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/app/images/favicon/favicon-16x16.png') }}">
        <link rel="manifest" href="{{ asset('bundles/app/images/favicon/manifest.json') }}">
        <meta name="msapplication-TileColor" content="#ffffff">
        <meta name="msapplication-TileImage" content="{{ asset('bundles/app/images/favicon/ms-icon-144x144.png') }}">
        <meta name="theme-color" content="#ffffff">
        <link rel="icon" type="image/x-icon" href="{{ asset('bundles/app/images/favicon/ms-icon-310x310.png') }}" />
    </head>
    <body>
        {% block body %}
            {% block menu %}
                <nav class="navbar navbar-fixed-top">
                    <div class="container-fluid">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="{{ path('homepage') }}"><img class="img-responsive img-circle logo" src="{{ asset('bundles/app/images/logo.png') }}"></a>
                        </div>

                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                {#{{ knp_menu_render('AppBundle:Builder:topMenu', { 'template': 'Menu/knp_menu.html.twig', 'currentAsLink': false, 'id':'navigation'}) }}#}
                                <li class="active"><a href="{{ path('homepage') }}">Home<span class="sr-only">(current)</span></a></li>
                                <li><a href="{{ path('about_us') }}">About Us</a></li>
                                <li><a href="{{ path('why_directrex') }}">Why Direct From US</a></li>
                                <li><a href="{% if app.user == null %}{{ path('products') }}
                                {% else %}{{ path('list') }}{% endif %}">Products</a></li>
                                <li><a href="{{ path('services') }}">Services</a></li>
                                <li><a href="{{ path('terms_and_conditions') }}">Terms And Conditions</a></li>
                                {% if app.user == null %}
                                <li><a href="{{ path('fos_user_security_login') }}">Login</a></li>
                                    {% else %}
                                        <li><a href="
                                        {% if app.user.userSettings %}
                                        {{ path('user_settings_show') }}{% else %}{{ path('user_settings') }}{% endif %}">Credit Application</a></li>
                                        <li><a href="{{ path('my_bag') }}" class="selected_infos">Bag&nbsp;</a></li>
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span class="glyphicon glyphicon-user"></span> &nbsp;
                                                {% if app.user.userSettings %}
                                                    {% set name = app.user.userSettings.tradeName|split(" ")%}
                                                    {{ name[0] }}{% else %}{{ app.user.lastname }}{% endif %}<span class="caret"></span></a>
                                            <ul class="dropdown-menu">
                                                {% if is_granted('ROLE_SUPER_ADMIN') %}
                                                    <li><a href="{{ path('sonata_admin_dashboard') }}"><span class="glyphicon glyphicon-book">&nbsp;Dashboard</span></a></li>
                                                {% endif %}
                                                <li><a href="{{ path('submit_order') }}"><span class="glyphicon glyphicon-screenshot"></span>&nbsp;Orders</a></li>
                                                {#<li><a href="{{ path('fos_user_profile_edit') }}"><span class="glyphicon glyphicon-edit"></span>&nbsp;Edit Profile</a></li>#}
                                                {#<li role="separator" class="divider"></li>#}
                                                {#<li><a href="{{ path('fos_user_profile_show') }}"><span class="glyphicon glyphicon-eye-open"></span>&nbsp;Profile</a></li>#}
                                                <li role="separator" class="divider"></li>
                                                <li><a href="{{ path('fos_user_security_logout') }}"><span class="glyphicon glyphicon-log-out"></span>&nbsp;Logout</a></li>
                                            </ul>
                                        </li>
                                {% endif %}
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </div><!-- /.container-fluid -->
                </nav>
            {% endblock %}
            {% block banner %}
                <div id="slider">
                    <figure>
                        <img src="{{ asset('bundles/app/images/slider/1.jpg') }}" alt>
                        <img src="{{ asset('bundles/app/images/slider/2.jpg') }}" alt>
                        {#{{ <img src="{{ asset('bundles/app/images/slider/3.jpg') }}" alt> }}#}
                        <img src="{{ asset('bundles/app/images/slider/4.jpg') }}" alt>
                        <img src="{{ asset('bundles/app/images/slider/5.jpg') }}" alt>
                    </figure>
                </div>
            {% endblock %}
            {% block content %}
            {% endblock %}
            {% block footer %}
                <footer class="footer">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-4 col-xs-6">
                                <ul class="footer-info-list">
                                    <li>AAROGYA MEDSURG LLC</li>
                                    <li><span>Toll Free:</span>1-888-975-8728</li>
                                    <li><span>Fax:</span>1-888-624-2851</li>
                                    <li><span>Direct:</span>1-909-682-8477</li>
                                    <li><span>Tel:</span>1-909-980-9900</li>
                                </ul>
                            </div>
                            <div class="col-md-4 col-xs-6">
                                <ul class="footer-info-list">
                                    <li>Working hours Mon to Fri 9-00am to 5-00pm PST</li>
                                    <li><span>Address:</span>8351 ELM AVE, #103</li>
                                    <li><span>City:</span>RANCHO CUCAMONGA</li>
                                    <li><span>State:</span>CA</li>
                                    <li><span>Zip:</span>91730</li>
                                </ul>
                            </div>
                            <div class="col-md-4 col-xs-12">
                                <ul class="footer-info-image">
                                    <li><p>Email: info@aamedllc.com</p></li>
                                </ul>
                                <ul class="footer-info-image">
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-rounded" src="{{ asset('bundles/app/images/certificates/1.png') }}">
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-thumbnail" src="{{ asset('bundles/app/images/certificates/2.png') }}">
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-thumbnail" src="{{ asset('bundles/app/images/certificates/3.png') }}">
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img class="img-responsive img-thumbnail" src="{{ asset('bundles/app/images/certificates/4.png') }}">
                                        </a>
                                    </li>
                                </ul>

                            </div>
                        </div>
                    </div>
                </footer>
            {% endblock %}
        {% endblock %}
        <script type="text/javascript" src="{{ asset('jquery_components/jquery/dist/jquery.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('jquery_components/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        <script type="text/javascript">
            var text = $('.dropdown-toggle').text();
            $('.dropdown-toggle').html('<span class="glyphicon glyphicon-user"></span> &nbsp;'+text+'&nbsp;<span class="caret"></span>')
        </script>
        {% if app.user %}
            <script type="text/javascript" src="{{ asset('bundles/app/js/main.js') }}"></script>
        {% endif %}
        {% block javascripts %}
        {% endblock %}
    </body>
</html>
